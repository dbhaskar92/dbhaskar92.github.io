	pmid = {37102999},
	pmcid = {PMC10140546},
	keywords = {Animals, Mice, Calcium, Epidermis, Cell Cycle, Calcium Signaling, Cell Cycle Checkpoints},
	pages = {e202302095},
	file = {Full Text:/Users/dbhaskar/Zotero/storage/X2IZMH2U/Moore et al. - 2023 - Cell cycle controls long-range calcium signaling i.pdf:application/pdf},
