	keywords = {Female, Humans, Epithelial Cells, Cell Communication, Cell Line, Cell Count, Cell Movement, collective migration, Epidermal Growth Factor, gelation, jamming, Mammary Glands, Human},
	pages = {17298--17306},
	file = {Full Text:/Users/dbhaskar/Zotero/storage/76ER2SR2/Leggett et al. - 2019 - Motility-limited aggregation of mammary epithelial.pdf:application/pdf},
